  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Debug",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Debug/CPPGL.exe",
      "symbolSearchPath": "C:\\Symbols;C:\\SymbolDir2",
      "console": "integratedTerminal",
      "logging": {
        "moduleLoad": false,
        "trace": true
      },
      "environment": [],
      "args": [],
      "visualizerFile": "${workspaceFolder}/my.natvis",
      "cwd": "${workspaceRoot}"
      // "showDisplayString": false
    }
  ]
}
